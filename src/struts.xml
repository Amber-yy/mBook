<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
"-//Aapche Software Foundation//DTD Struts Configuration2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
<constant name="struts.objectFactory" value="spring"/>

<package name="default" extends="struts-default">
<action name="signUp1" class="loginRegistAction"  method="checkInfo">
<result name="success">/signUp.jsp</result>
<result name="input">/signUp.jsp</result>
</action>

<action name="login" class="loginRegistAction"  method="login">
	<result name="success" type="redirect">/index.jsp</result>
	<result name="admin"  type="redirect">/admin.jsp</result>
	<result name="input">/login.jsp</result>

</action>
</package>
</struts>